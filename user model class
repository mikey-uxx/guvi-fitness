public class User {
    private int userId;
    private String name;
    private String password;
    private int regularityPoints;
    private int timeSpent;
 
    // Constructors
    public User() {}
 
    public User(int userId, String name, String password, int regularityPoints, int timeSpent) {
        this.userId = userId;
        this.name = name;
        this.password = password;
        this.regularityPoints = regularityPoints;
        this.timeSpent = timeSpent;
    }
 
    // Getters and Setters
    public int getUserId() {
        return userId;
    }
 
    public void setUserId(int userId) {
        this.userId = userId;
    }
 
    public String getName() {
        return name;
    }
 
    public void setName(String name) {
        this.name = name;
    }
 
    public String getPassword() {
        return password;
    }
 
    public void setPassword(String password) {
        this.password = password;
    }
 
    public int getRegularityPoints() {
        return regularityPoints;
    }
 
    public void setRegularityPoints(int regularityPoints) {
        this.regularityPoints = regularityPoints;
    }
 
    public int getTimeSpent() {
        return timeSpent;
    }
 
    public void setTimeSpent(int timeSpent) {
        this.timeSpent = timeSpent;
    }
 }
 